{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:@typescript-eslint/recommended",
        "plugin:@next/next/recommended",
        "plugin:react/recommended",
        "eslint:recommended",
        "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "react"
    ],
    "rules": {
        "no-undef": 0,
        "no-unused-vars": 0,
        "@typescript-eslint/no-explicit-any": 0,
        "no-useless-catch": 0
    }
}
